def is_valid(s):
    stack = []
    asterisks = []

    for char in s:
        if char == '(':
            stack.append(char)
        elif char == '*':
            asterisks.append(char)
        else:  # char == ')'
            if stack:
                stack.pop()
            elif asterisks:
                asterisks.pop()
            else:
                return "false"  # Return "false" instead of False

    while stack and asterisks:
        if stack[-1] > asterisks[-1]:
            return "false"  # Return "false" instead of False
        stack.pop()
        asterisks.pop()

    return "true" if len(stack) == 0 else "false"  # Return "true" or "false" instead of True or False


# Read input
s = input()

# Call the function and print the result
result = is_valid(s)
print(result)
